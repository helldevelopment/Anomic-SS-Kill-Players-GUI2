X.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.PlayerGui.KillPlayersGUI:Destroy()
end)

Teleport.MouseButton1Click:Connect(function(plr)
	if focused ~= nil then
		local target = game.Workspace:FindFirstChild(focused)
		if target and target:FindFirstChild("HumanoidRootPart") then
			local targetPosition = target.HumanoidRootPart.Position
			local newCFrame = CFrame.new(targetPosition)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = newCFrame
		end
	end
end)

TextButton.MouseButton1Click:Connect(function()
	if ScrollingFrame.Visible == true then
		ScrollingFrame.Visible = false
	else
		ScrollingFrame.Visible = true
	end
end)

local playerButtons = {}


while true do
	wait(1)
	for i,players in pairs(game.Players:GetChildren()) do
		if players.Name ~= game.Players.LocalPlayer.Name and not playerButtons[players.Name] then
			local Textbutton = Instance.new("TextButton")
			Textbutton.Parent = ScrollingFrame
			Textbutton.BackgroundColor3 = Color3.fromRGB(80,80,80)
			Textbutton.TextScaled = true
			Textbutton.Font = Enum.Font.SciFi
			Textbutton.Text = players.Name
			Textbutton.Name = players.Name

			Textbutton.MouseButton1Click:Connect(function()
				focused = players.Name
				TextButton.Text = players.Name
				print(focused)
			end)

			playerButtons[players.Name] = Textbutton
		end
	end
end

Kill.MouseButton1Click:Connect(function()
	if iskilling == false then
		local player = game.Workspace:FindFirstChild(focused)
		local oldposition = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
		local playerpositon = player:FindFirstChild("HumanoidRootPart").CFrame
		local newCFrame = CFrame.new(playerpositon)
		local head = player:FindFirstChild("Head").Position
		local new = CFrame.new(0,5,0)
		local gun = game.Players.LocalPlayer.Character:FindFirstChild("Handgun") or game.Players.LocalPlayer.Character:FindFirstChild("Riot PDW") or game.Players.LocalPlayer.Character:FindFirstChild("Revolver")


		local args = {
			[1] = Vector3.new(head),
			[2] = game:GetService("Players"):WaitForChild(focused).Character.Head,
			[3] = game:GetService("Players").LocalPlayer.Character:FindFirstChild(gun).Shot,
			[4] = player.Head,
			[5] = Enum.NormalId.Front
		}

		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = newCFrame + new
		
		repeat
			wait(0.2)
			game:GetService("ReplicatedStorage"):WaitForChild("_CS.Events"):WaitForChild("RayDrawer"):FireServer(unpack(args))
		until game:GetService("Players"):WaitForChild(focused).Character.Humanoid.Health < 1
		
		
		iskilling = true
		
	else
		notify("Anomic SS", "Please wait until the killing process is over, otherwise the script would break.")
	end

	
	
end)




game.Players.PlayerRemoving:Connect(function(player)
	print(player.Name)
	local button = playerButtons[player.Name]
	if button then
		button:Destroy()
		playerButtons[player.Name] = nil -- Remove the button from thLayoute table
		local pl = game.Players.LocalPlayer.PlayerGui.KillPlayersGUI..ScrollingFrame:FindFirstChild(player.Name)
		pl:Destroy()
	end
end)





game.Players.PlayerAdded:Connect(function(plr)
	local Textbutton = Instance.new("TextButton")
	Textbutton.Parent = ScrollingFrame
	Textbutton.BackgroundColor3 = Color3.fromRGB(80,80,80)
	Textbutton.TextScaled = true
	Textbutton.Font = Enum.Font.SciFi
	Textbutton.Text = plr.Name
	Textbutton.Name = plr.Name

	Textbutton.MouseButton1Click:Connect(function()
		focused = plr.Name
		TextButton.Text = plr.Name
		print(focused)
	end)

	playerButtons[plr.Name] = Textbutton
end)
